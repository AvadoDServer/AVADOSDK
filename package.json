{
  "name": "avadosdk",
  "version": "0.4.1",
  "description": "avadosdk is a tool to make the creation of new avado packages as simple as possible. It helps to initialize and publish in ethereum blockchain",
  "main": "avadosdk.js",
  "scripts": {
    "test": "mocha \"./{,!(node_modules)/**}/*.test.js\" ",
    "test:file": "mocha ",
    "lint": "./node_modules/.bin/eslint . --ext .js --fix",
    "tag-and-publish": "npm version patch -m 'Bump version' && git push origin master && git push --tags",
    "pre-commit": "npm run lint && npm run test"
  },
  "bin": {
    "avadosdk": "./src/avadosdk.js"
  },
  "keywords": [
    "cli",
    "avado",
    "ethereum"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "@dappnode/dnp-manifest": "^0.0.4",
    "@octokit/rest": "^16.27.3",
    "async-retry": "^1.2.3",
    "chalk": "^2.4.2",
    "ethjs": "^0.4.0",
    "ethjs-abi": "^0.2.1",
    "ethjs-ens": "^2.0.1",
    "ethjs-util": "^0.1.6",
    "execa": "^1.0.0",
    "figlet": "^1.2.3",
    "form-data": "^3.0.0",
    "got": "^10.5.5",
    "image-size": "^0.8.1",
    "inquirer": "^6.2.1",
    "ipfs-http-client": "^49.0.4",
    "js-yaml": "^3.12.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.24",
    "semver": "^5.6.0",
    "tar-fs": "^2.0.0",
    "timestring": "^5.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
